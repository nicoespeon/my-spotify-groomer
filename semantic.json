{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../web/static/vendor/semantic/",
      "uncompressed": "../web/static/vendor/semantic/components/",
      "compressed": "../web/static/vendor/semantic/components/",
      "themes": "../web/static/vendor/semantic/assets/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "grid",
    "list",
    "table",
    "image",
    "loader",
    "segment",
    "dimmer",
    "menu",
    "header",
    "form",
    "label",
    "dropdown",
    "icon",
    "divider",
    "transition",
    "popup"
  ],
  "version": "2.2.13"
}
